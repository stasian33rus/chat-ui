{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "typeRoots": ["./typings"],
    "esModuleInterop": true,
    "sourceMap": true
  },
  "include": ["src/**/*", "typings/*"]
}
